package encapsulation;

public class Train {
	private int snum;
	private String code;
	private String Station;
	private double Arrival;
	private double Departure;
	public Train(int snum,String code,String Station,double Arrival,double Departure) {
		this.snum=snum;
		this.code=code;
		this.Station=Station;
		this.Arrival=Arrival;
		this.Departure=Departure;	
	}
	public void printStatement() {
		System.out.printf("%d %10s %12s %10.2d %10.2d",snum,code,Station,Arrival,Departure);
	}
	public static void main(String args[]) {
		System.out.println("yasvanthapur Delhi Sari Rohilla AC Duronto Express (12213)");
		System.out.println();
		System.out.println("--------------------------------------------");
		System.out.println("#  code      station     Arr      Dep");
		System.out.println("--------------------------------------------");
	}
}
		
