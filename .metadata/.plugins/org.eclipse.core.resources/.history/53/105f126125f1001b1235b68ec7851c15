
 public class Encapsulation {
	
		private int num;
		private String code;
		private String  Station;
		private double Arr;
		private double Dep;
		public Encapsulation(int num, String code,String Station,double Arr,double Dep) {
			this.num=num;
			this.code=code;
			this.Station=Station;
			this.Arr=Arr;
			this.Dep=Dep;		
		}
		
		
		

		